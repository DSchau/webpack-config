{
  "name": "webpack-config",
  "version": "1.0.0",
  "main": "./dist/index",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc",
    "clean": "del-cli \"dist/**/*\"",
    "precommit": "lint-staged",
    "prettier": "prettier --write --single-quote --parser typescript \"src/**/*.ts\""
  },
  "dependencies": {
    "autoprefixer": "~7.1.1",
    "babel-core": "~6.25.0",
    "babel-plugin-syntax-dynamic-import": "~6.18.0",
    "babel-preset-env": "~1.5.2",
    "css-loader": "~0.28.4",
    "cssnano": "~3.10.0",
    "extract-text-webpack-plugin": "~2.1.2",
    "file-loader": "~0.11.2",
    "html-webpack-plugin": "~2.28.0",
    "name-all-modules-plugin": "~1.0.1",
    "node-sass": "~4.5.3",
    "object.omit": "~2.0.1",
    "offline-plugin": "~4.8.1",
    "postcss": "~6.0.2",
    "postcss-loader": "~2.0.5",
    "pug": "~2.0.0-rc.2",
    "pug-loader": "~2.3.0",
    "sass-loader": "~6.0.5",
    "style-ext-html-webpack-plugin": "~3.4.1",
    "style-loader": "~0.18.2",
    "url-loader": "~0.5.9",
    "webpack": "~2.6.1",
    "webpack-config-assign": "~1.0.0"
  },
  "devDependencies": {
    "@types/node": "~7.0.31",
    "del-cli": "~1.0.0",
    "husky": "~0.13.4",
    "lint-staged": "~3.6.1",
    "prettier": "~1.4.4",
    "typescript": "~2.3.4"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write --single-quote --parser typescript",
      "git add"
    ]
  }
}
